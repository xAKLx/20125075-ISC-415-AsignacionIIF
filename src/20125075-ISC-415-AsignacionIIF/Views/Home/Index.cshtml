@{
    ViewData["Title"] = "Home Page";
}

<div class="row fullheight">
    <div id="contactsContainer" class="col-md-3">
        <div id="contacts" class="col-md-12">
            @for (int i = 0; i < 20; i++)
            {
                <div class="row contact">
                    <div class="col-md-12 contactName">
                        @("contact: #" + i.ToString())
                    </div>
                </div>
            }

        </div>
    </div>
    <div id="chatContainer" class="col-md-9">
        <div id="chat" class="col-md-12">

        </div>
    </div>
</div>
